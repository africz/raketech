./auth0 apps create \
  --name "Flags" \
  --type "regular" \
  --auth-method "post" \
  --callbacks "http://be.localhost/callback" \
  --logout-urls "http://be.localhost" \
  --reveal-secrets \
  --no-input \
  --json > .auth0.app.json

# Result:

# === dev-pna8qz71k28j5dn3.eu.auth0.com application created


